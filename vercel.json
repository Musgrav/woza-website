{
  "routes": [
    {
      "src": "/.well-known/apple-developer-merchantid-domain-association",
      "dest": "/.well-known/apple-developer-merchantid-domain-association",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": false
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1",
      "continue": false
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

